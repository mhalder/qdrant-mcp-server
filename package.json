{
  "name": "qdrant-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for semantic search using local Qdrant vector database",
  "type": "module",
  "bin": {
    "qdrant-mcp-server": "./build/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:providers": "node scripts/verify-providers.js",
    "type-check": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "keywords": [
    "mcp",
    "qdrant",
    "vector-search",
    "semantic-search",
    "embeddings"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@qdrant/js-client-rest": "^1.12.0",
    "bottleneck": "^2.19.5",
    "cohere-ai": "^7.19.0",
    "openai": "^4.77.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
